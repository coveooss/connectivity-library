{
  "Services": [
    {
      "Authentication": {
        "OAuth": {
          "Headers": {
            "Content-Type": "application/json"
          },
          "Query": {
            "RefreshUrl": "https://www.googleapis.com/oauth2/v4/token",
            "Method": "POST",
            "Parameters": {
              "grant_type": {
                "Type": "Payload",
                "Value": "refresh_token"
              },
              "refresh_token": {
                "Type": "Payload",
                "Value": "@RefreshToken",
                "IsRefreshToken": true
              },
              "client_id": {
                "Type": "Payload",
                "Value": "@ClientID"
              },
              "client_secret": {
                "Type": "Payload",
                "Value": "@ClientSecret"
              },
              "redirect_uri": {
                "Type": "Payload",
                "Value": "http://localhost"
              }
            }
          },
          "Response": {
            "AccessToken": "access_token",
            "SupportsRefreshToken": "false",
            "ExpiresIn": "expires_in",
            "TokenType": "token_type"
          }
        }
      },
      "Url": "https://sheets.googleapis.com/v4/spreadsheets/<id>",
      "Endpoints": [
        {
          "Path": "/values/<<ENTER_TARGET_SHEETNAME>>!<<ENTER_TARGET_RANGE>>",
          "Method": "GET",
          "QueryParameters": {
            "majorDimension": "ROWS"
          },
          "ItemType": "<type>",
          "ItemPath": "values",
          "Uri": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]",
          "ClickableUri": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]",
          "Title": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]",
          "Metadata": {
            "metadata1": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]",
            "metadata2": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]",
            "metadata3": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]",
            "metadata4": "%[$[<<ENTER_DATAPOINT_COLUMN_NUMBER>>]]"
          }
        }
      ]
    }
  ]
}