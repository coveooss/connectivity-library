{
  "Url": "https://graph.facebook.com/",
  "Paging": {
    "PageSize": 1000,
      "Parameters": {
        "Limit": "limit"
    },
    "OffsetType": "url",
    "NextPageKey": "paging.next"
  },
"Identities": {
    "MembersGroup": {
      "PermissionSubQueries": [{
        "Path": "%[coveo_parent.groupid]/members",
        "ItemPath": "data",
        "Method": "GET",
        "QueryParameters": {
          "token": "@ApiKey",
          "fields": "id,email,name"
        },
        "Headers": {
          "Authorization": "Bearer @ApiKey"
        },
        "Name": "%[email]",
        "Type": "User"
       
        
      }]
    },
    "MembersCommunity": {
      "PermissionSubQueries": [{
        "Path": "community/members",
        "ItemPath": "data",
        "Method": "GET",
        "QueryParameters": {
          "token": "@ApiKey",
          "fields": "id,email,name"
        },
        "Headers": {
          "Authorization": "Bearer @ApiKey"
        },
        "Name": "%[email]",
        "Type": "User"
       
        
      }]
    }
 }
}