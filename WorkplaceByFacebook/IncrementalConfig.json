            "id": "%[id]",
            "groupid": "%[id]",
            "mediaurl": "%[cover.source]",
            "iconurl": "%[icon]"
          },
          "IndexingAction": {
            "ActionOnItem": "Ignore",
            "Condition": "1 == 1"
          },
          "ProcessingAction": {
            "ActionOnItem": "Process",
            "Condition": "1 == 1"
          },
          "Permissions": [
            {
              "PermissionsSets": [
